    "typescript",
    "components",
    "hooks",
    "ui-library",
    "simplygrow"
  ],
  "author": "Stephen Scholtz",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "typescript": ">=3.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-react": "^7.24.7",
    "@mui/x-data-grid": "^7.28.0",
