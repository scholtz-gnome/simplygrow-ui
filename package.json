{
  "name": "simplygrow-ui",
  "version": "0.7.26",
  "description": "A library of React components, hooks and utility functions built with TypeScript and CSS Modules.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
