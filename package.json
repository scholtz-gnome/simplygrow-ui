    "rollup-preserve-directives": "^1.1.1",
    "size-limit": "^9.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.4.1",
    "@rollup/plugin-terser": "^0.4.3",
    "fuse.js": "^7.1.0",
